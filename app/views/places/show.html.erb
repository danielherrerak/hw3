<h1> <%= @place["name"]%> </h1>

<ul>
    <% for post in @posts %>
            <h2> <%= post["title"]%> </h2>
            <p> <%= post["posted_on"] %> </p>
            <p> <%= post["description"] %> </p>
            <p> <%= "---------------------------------------" %> </h5>
    <% end %>
</ul>

<p> 
    <a href = "/posts/new?place_id=<%= @place["id"] %>"> New Post for <%= @place["name"]%> </a> 
</p> 

<h4>
    <b> <a href = "/places" > Back to all Places </a></b>
</h4>